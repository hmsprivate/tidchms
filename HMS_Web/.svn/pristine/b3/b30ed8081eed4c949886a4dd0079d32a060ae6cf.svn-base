.ui-resizable-helper { 
	border: 2px solid #aaaaaa;
}

.sub-window {
	border: solid 1px #a0a0a0;
	-webkit-box-shadow: 0 2px 5px rgba(0,0,0,0.2); 
	box-shadow:0 2px 5px rgba(0,0,0,0.2);
	background-color: #fff;
}
.sub-window-select {border:solid 3px #94b42b;}/* 2015-12-10 ADD : sub-window 선택시 */

.sub-top {
	position:relative;
	height: 25px;
	border-top: solid 1px #fff;
	background: #f6f6f6;
	font-weight: 500;
}
.sub-top h3.title {
	height:23px;
	line-height:23px;
	padding:0 0 0 15px;
	color: #8a8b90;
}
.sub-top-select { 
	background: linear-gradient(to bottom,#e4e4e4,#d5d5d5);
}
.sub-top-select h3.title { 
	height:23px;
	line-height:23px;
	padding:0 0 0 15px;
	background: url("../../images/mdi/bg_title_select.png") no-repeat 6px 50%;
	color: #484a54;
}
.sub-bottom {
	height: 16px;
	margin: 0;
	padding: 0;
}
.ui-resizable-se {
	width: 16px !important;
	height: 16px !important;
	background-image:url("../../images/mdi/resize.png") !important;
}
.sub-button {
	height: 16px;
	width: 16px;
	border: 0;
	background-color: transparent;
	position:absolute;
	top:5px;
}
.minimize-window {
	background-image:url("../../images/mdi/minimize.png");
}
.normal-window {
	background-image:url("../../images/mdi/restore.png");
}
.maximize-window {
	background-image:url("../../images/mdi/maximize.png");
}
.close-window {
	background-image:url("../../images/mdi/close.png");
}
.panel-controls {
	border: solid 1px #ddd;
	border-radius: 3px;
}

.tooltip-inner {
	max-width: 800px !important;
	width: auto;
}
.panel-header-small {
	padding-top:0px !important;
}

.divider {
	background-color: #eeeeee
}

input[type=file] {
  position: absolute;
  top: 0;
  right: 0;
  min-width: 100%;
  min-height: 100%;
  font-size: 100px;
  text-align: right;
  filter: alpha(opacity=0);
  opacity: 0;
  background: red;
  cursor: inherit;
  display: block;
}

.taskbar-button-normal, .taskbar-button-normal:hover  {
	width:145px; 
	height:27px;
	border:1px solid #aeaeae;
	background:linear-gradient(to bottom,#e4e4e4,#d6d6d6); background:-webkit-linear-gradient(#e4e4e4,#d6d6d6); 
	color: #484a54;
	-moz-box-shadow: inset 0 1px 0 0 rgba(237, 237, 237, 1), 0px 1px 0px 0px rgba(237, 237, 237, 1);
	-webkit-box-shadow: inset 0 1px 0 0 rgba(237, 237, 237, 1), 0px 1px 0px 0px rgba(237, 237, 237, 1);
	box-shadow: inset 0 1px 0 0 rgba(237, 237, 237, 1), 0px 1px 0px 0px rgba(237, 237, 237, 1);
}

.taskbar-button-hidden, .taskbar-button-hidden:hover {
	width:145px; 
	height:27px;
	border:1px solid #aeaeae;
	background:#c2c2c2;
	-moz-box-shadow: inset 0 1px 0 0 rgba(187, 187, 187, 1), 0px 1px 0px 0px rgba(237, 237, 237, 1);
	-webkit-box-shadow: inset 0 1px 0 0 rgba(187, 187, 187, 1), 0px 1px 0px 0px rgba(237, 237, 237, 1);
	box-shadow: inset 0 1px 0 0 rgba(187, 187, 187, 1), 0px 1px 0px 0px rgba(237, 237, 237, 1);
}

.taskbar-button-hidden .taskbar-button-label, .taskbar-button-hidden:hover .taskbar-button-label {color:#797979;}

.taskbar-button-hidden .taskbar-button-close, .taskbar-button-hidden:hover .taskbar-button-close {opacity:.5;}

.taskbar-button-select, .taskbar-button-select:hover {
	width:145px; 
	height:27px;
	border:1px solid #9a9a9a;
	background:linear-gradient(to bottom,#ffffff,#e9e9e9); background:-webkit-linear-gradient(#ffffff,#e9e9e9); 
	color: #484a54;
	-moz-box-shadow: inset 0 1px 0 0 rgba(237, 237, 237, 1), 0px 1px 0px 0px rgba(202, 202, 202, 1);
	-webkit-box-shadow: inset 0 1px 0 0 rgba(237, 237, 237, 1), 0px 1px 0px 0px rgba(202, 202, 202, 1);
	box-shadow: inset 0 1px 0 0 rgba(237, 237, 237, 1), 0px 1px 0px 0px rgba(202, 202, 202, 1);
}
/* taskbar-button-label 2016-06-30 DEL : font-weight:700, ADD : text-align:left */
.taskbar-button-label {
	overflow: hidden; 
	white-space: nowrap; 
	text-overflow: ellipsis;
	border: 0;
	background-color: transparent;
	width: 117px;
    height: 25px;
	margin:0 0 0 10px;
	text-align:left;
}

.taskbar-button-close {
	height: 16px;
	width: 16px;
	border: 0;
	background-color: transparent;
	background: url("../../images/mdi/close.png");
}
/* common.css에 tooltip과 동일 디자인 */
.taskbar-context-menu {position:absolute; border:1px solid #d3d3d3; border-radius:2px; -webkit-box-shadow:0 0 3px 0 #d4d4d4; box-shadow:0 0 3px 0 #d4d4d4; background:#fff;}

/* common.css List Basic와 동일 디자인 */
.taskbar-context-menu-list {overflow:hidden;}
.taskbar-context-menu-list li {line-height:18px; background:#fff;}
.taskbar-context-menu-list li a {display:block; padding:3px 5px; color:#6b6b6b; text-align:left;}
.taskbar-context-menu-list li a:hover,
.taskbar-context-menu-list li a:focus {background:#e9ecff; color:#262626;}
